    <Grid>
        <Label x:Name="lNombre" Content="Cliente" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top"/>
        <ComboBox x:Name="cNombres" HorizontalAlignment="Left" Margin="78,13,0,0" VerticalAlignment="Top" Width="322"/>
        <Label x:Name="label2" Content="Mes" HorizontalAlignment="Left" Margin="83,42,0,0" VerticalAlignment="Top" RenderTransformOrigin="0,0.423"/>
