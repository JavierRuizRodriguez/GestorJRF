        <TextBox x:Name="tAño" HorizontalAlignment="Left" Height="22" Margin="258,46,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="69" RenderTransformOrigin="0.872,1.435"/>
        <Label x:Name="label3" Content="Año" HorizontalAlignment="Left" Margin="221,42,0,0" VerticalAlignment="Top" RenderTransformOrigin="1.219,0.577"/>
        <Button x:Name="bGenerarEstadistica" Content="GENERAR ESTADÍSTICA" HorizontalAlignment="Left" Margin="10,80,0,0" VerticalAlignment="Top" Width="390" Height="31" Click="bGenerarEstadistica_Click"/>
